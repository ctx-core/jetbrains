{
	"name": "@ctx-core/jetbrains",
	"version": "7.2.82",
	"description": "@ctx-core jetbrains product helpers",
	"homepage": "https://github.com/ctx-core/jetbrains#readme",
	"bin": {
		"idea-exclude-config": "bin/idea-exclude-config.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"type": "module",
	"types": "./src/index.d.ts",
	"svelte": "./src/index.js",
	"exports": {
		".": "./src/index.js",
		"./package.json": "./package.json"
	},
	"dependencies": {
		"@ctx-core/array": "^26.15.1",
		"@ctx-core/cli-args": "^11.3.38",
		"@ctx-core/queue": "^7.9.1",
		"dir-glob": "^3.0.1",
		"globby": "^13.2.2",
		"path-type": "^5.0.0"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^3.12.8",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/jetbrains.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/jetbrains/issues"
	},
	"sideEffects": false
}