{
	"name": "@ctx-core/jetbrains",
	"version": "7.1.18",
	"description": "@ctx-core jetbrains product helpers",
	"homepage": "https://github.com/ctx-core/jetbrains#readme",
	"bin": {
		"idea-exclude-config": "bin/idea-exclude-config.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.41",
		"@ctx-core/cli-args": "workspace:^11.0.19",
		"@ctx-core/queue": "workspace:^6.1.16",
		"globby": "^12.0.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.121",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/jetbrains.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/jetbrains/issues"
	}
}
