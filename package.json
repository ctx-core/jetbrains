{
	"name": "@ctx-core/jetbrains",
	"version": "7.3.179",
	"description": "@ctx-core jetbrains product helpers",
	"keywords": [
		"ctx-core",
		"jetbrains",
		"idea",
		"pycharm",
		"rubymine",
		"webstorm"
	],
	"homepage": "https://github.com/ctx-core/jetbrains#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/jetbrains/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/jetbrains.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-jetbrains@briantakita.me"
	},
	"bin": {
		"idea-exclude-config": "bin/idea-exclude-config.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"bin",
		"excludeFolder_a"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"dependencies": {
		"ctx-core": "^7.2.3",
		"dir-glob": "^3.0.1",
		"globby": "^14.0.1",
		"path-type": "^5.0.0"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.9.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}