{
	"name": "@ctx-core/jetbrains",
	"version": "6.0.4",
	"description": "@ctx-core jetbrains product helpers",
	"homepage": "https://github.com/ctx-core/jetbrains#readme",
	"bin": {
		"idea-exclude-config": "bin/idea-exclude-config.js"
	},
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@"
	},
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"dependencies": {
		"@ctx-core/cli-args": "workspace:^9.0.5",
		"@ctx-core/queue": "workspace:^5.0.3",
		"esm": "^3.2.25",
		"globby": "^11.0.4"
	},
	"devDependencies": {
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}