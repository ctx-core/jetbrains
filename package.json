{
	"name": "@ctx-core/jetbrains",
	"version": "7.1.2",
	"description": "@ctx-core jetbrains product helpers",
	"homepage": "https://github.com/ctx-core/jetbrains#readme",
	"bin": {
		"idea-exclude-config": "bin/idea-exclude-config.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@"
	},
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.25",
		"@ctx-core/cli-args": "workspace:^11.0.2",
		"@ctx-core/queue": "workspace:^6.1.1",
		"globby": "^12.0.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.110",
		"typescript": "^4.5.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}