{
	"name": "@ctx-core/jetbrains",
	"version": "7.1.19",
	"description": "@ctx-core jetbrains product helpers",
	"homepage": "https://github.com/ctx-core/jetbrains#readme",
	"bin": {
		"idea-exclude-config": "bin/idea-exclude-config.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@"
	},
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.1.0",
		"@ctx-core/cli-args": "workspace:^11.0.19",
		"@ctx-core/queue": "workspace:^6.1.17",
		"globby": "^12.0.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.124",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/jetbrains.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/jetbrains/issues"
	}
}
